<scenario>
  <name>Shipping Rules Test - One Cart.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>shipping-rules</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>0.00</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>$sc</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>Row 1 Shipping Rules</ruleName>
      <expectedCount>1</expectedCount>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>shippingTotal</fieldName>
          <expected>2.99</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>24.00</value>
          <nature>4</nature>
        </field-data>
        <field-data>
          <name>shippingTotal</name>
          <value>0.00</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>$sc</name>
      <isModify>true</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>Row 1 Shipping Rules</ruleName>
      <expectedCount>1</expectedCount>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>shippingTotal</fieldName>
          <expected>2.99</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
      </fieldValues>
      <name>$sc</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>com.meliorix.coolstore</packageName>
  <imports>
    <imports/>
  </imports>
</scenario>